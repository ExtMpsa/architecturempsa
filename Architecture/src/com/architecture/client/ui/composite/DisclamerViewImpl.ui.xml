<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.architecture.client.ui.widget">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel styleName="disclaimer wrapper" ui:field="wrapper">
		<g:HTMLPanel ui:field="content" styleName="content">
			<g:HTMLPanel ui:field="cellLeft" styleName="left">
				<g:Label text="disclaimer" ui:field="text"/>
				<p1:Anchor text="En savoir plus" ui:field="moreInformation"/>
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="cellRight" styleName="right">
				<g:Button text="OK" ui:field="ok"/>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 