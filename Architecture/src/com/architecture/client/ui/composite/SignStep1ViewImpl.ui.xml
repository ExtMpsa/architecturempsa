<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel styleName="gwt-SignStep">
	<g:HTMLPanel styleName="gwt-SignStep-Title">
		<h3 ui:field="step">Step 1</h3>
		<g:Button text="Modify" ui:field="modify"/>
	</g:HTMLPanel>
	<g:HorizontalPanel ui:field="content">
		<g:VerticalPanel>
			<g:Label text="Name" ui:field="name"/>
			<g:TextBox ui:field="nameValue"/>
			<g:Label text="First Name" ui:field="firstName"/>
			<g:TextBox ui:field="firstNameValue"/>
		</g:VerticalPanel>
		<g:VerticalPanel>
			<g:Label text="Mail" ui:field="mail"/>
			<g:TextBox ui:field="mailValue"/>
			<g:Label text="PSA Entity" ui:field="psaEntity"/>
			<g:TextBox ui:field="psaEntityValue"/>
		</g:VerticalPanel>
	</g:HorizontalPanel>
	<g:Button text="Validate" ui:field="validate">Valider</g:Button>
	</g:HTMLPanel>
</ui:UiBinder> 