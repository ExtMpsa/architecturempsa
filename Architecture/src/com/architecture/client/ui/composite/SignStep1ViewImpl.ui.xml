<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.architecture.client.ui.widget" xmlns:p2="urn:import:com.architecture.client.ui.composite">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel styleName="gwt-SignStep">
	<g:HTMLPanel styleName="gwt-SignStep-Title">
		<h3 ui:field="step">Step 1</h3>
		<g:Button text="Modify" ui:field="modify"/>
	</g:HTMLPanel>
	<g:Grid styleName="gwt-Grid" ui:field="content">
		<g:row>
			<g:customCell>
				<p2:FieldComposite ui:field="lastName"/>
			</g:customCell>
			<g:customCell>
				<p2:FieldComposite ui:field="mail"/>
			</g:customCell>
		</g:row>
		<g:row>
			<g:customCell>
				<p2:FieldComposite ui:field="firstName"/>
			</g:customCell>
			<g:customCell>
				<p2:FieldComposite ui:field="psaEntity"/>
			</g:customCell>
		</g:row>
		<g:row>
			<g:customCell>
				<g:Button text="Validate" ui:field="validate">Valider</g:Button>
			</g:customCell>
			<g:customCell>
				<p1:Anchor ui:field="validate1" text="Validate" styleName="gwt-Button"/>
			</g:customCell>
		</g:row>
	</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 