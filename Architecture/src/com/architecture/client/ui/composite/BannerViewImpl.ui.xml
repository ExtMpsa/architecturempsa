<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='res' type='com.architecture.client.resources.ResourcesBanner'/>
	<ui:style>
	</ui:style>
	<g:HTMLPanel styleName="banner wrapper" ui:field="wrapper">
		<g:HTMLPanel ui:field="content" styleName="content">
			<g:HTMLPanel ui:field="left" styleName="cell left">
				<a ui:field="citroenLink" target="_blank">
					<g:Image resource="{res.logoCitroen}" ui:field="citroenLogo"/>
				</a>
				<a ui:field="dsLink" target="_blank">
					<g:Image resource="{res.logoDs}" ui:field="dsLogo"/>
				</a>
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="title" tag="h1">Formation Google Analytics PSA</g:HTMLPanel>
			<g:HTMLPanel ui:field="right" styleName="cell right">
				<a ui:field="peugeotLink" target="_blank">
					<g:Image resource="{res.logoPeugeot}" ui:field="peugeotLogo"/>
				</a>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 