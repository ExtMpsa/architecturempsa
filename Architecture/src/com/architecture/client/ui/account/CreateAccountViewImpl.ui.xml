<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel styleName="account" ui:field="panel">
		<h2 ui:field="createAccount">Create Account</h2>
		<g:TextBox ui:field="login"/>
		<g:Button ui:field="helpLogin" styleName="gwt-Button help-DataLayer"/>
		<g:Label text="erreur Client" ui:field="loginErrorClient" styleName="gwt-Label-Error"/>
		<g:Label text="erreur Serveur" ui:field="loginErrorServer" styleName="gwt-Label-Error"/>
		<g:Button ui:field="next" text="Next"/>
		<g:DecoratedPopupPanel ui:field="asideNext" styleName="gwt-Popup-Aside" autoHideEnabled="true" autoHideOnHistoryEventsEnabled="true">
			<g:HTMLPanel>
				<code>
					<pre ui:field="preNext">
dataLayer.push({
	event : event,
	eventCategory : category,
	eventAction : action,
	eventLabel : label
});
					</pre>
				</code>
			</g:HTMLPanel>
		</g:DecoratedPopupPanel >
	</g:HTMLPanel>
</ui:UiBinder> 