<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel styleName="account wrapper" ui:field="wrapper" tag="article">
		<g:HTMLPanel ui:field="content" styleName="content">
			<g:HTMLPanel ui:field="title" tag="h2">Sign Up</g:HTMLPanel>
			<g:PasswordTextBox ui:field="password"/>
			<g:Label text="erreur sizeMin" ui:field="sizeMinError" styleName="gwt-Label-Error"/>
			<g:Label text="erreur sizeMax" styleName="gwt-Label-Error" ui:field="sizeMaxError"/>
			<g:Label text="erreur no digit" styleName="gwt-Label-Error" ui:field="noDigitError"/>
			<g:Label text="erreur no lowercase" styleName="gwt-Label-Error" ui:field="noLowercaseError"/>
			<g:Label text="erreur no uppercase" styleName="gwt-Label-Error" ui:field="noUppercaseError"/>
			<g:Label text="erreur no special" styleName="gwt-Label-Error" ui:field="noSpecialError"/>
			<g:Label text="erreur no whitespace" ui:field="whitespaceError" styleName="gwt-Label-Error"/>
			<g:Button ui:field="verify" text="Verify"/>
			<g:PasswordTextBox ui:field="passwordVerify"/>
			<g:Label text="mot de passe pas identique" ui:field="passwordVerifyError" styleName="gwt-Label-Error"/>
			<g:Label text="erreur d&apos;enregistrement du compte" styleName="gwt-Label-Error" ui:field="createError"/>
			<g:Button ui:field="create" text="Create"/>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 