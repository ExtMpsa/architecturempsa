<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel styleName="account-Parameter wrapper" ui:field="wrapper" tag="article">
		<g:HTMLPanel ui:field="content" styleName="content">
			<g:HTMLPanel ui:field="title" tag="h2">Account Setting</g:HTMLPanel>
			<g:HTMLPanel ui:field="gtm" styleName="gtmContent">
				<g:InlineLabel text="GTM Id" ui:field="gtmIdLabel"/>
				<g:InlineLabel ui:field="gtmIdValue" styleName="value"/>
				<g:TextBox ui:field="gtmIdInput"/>
				<g:Button text="Modifier" ui:field="editGtm"/>
				<g:Button text="Enregistrer" ui:field="saveGtm"/>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 