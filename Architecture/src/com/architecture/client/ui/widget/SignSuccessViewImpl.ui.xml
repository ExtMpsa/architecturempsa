<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<g:HTMLPanel styleName="gwt-SignStep-Title">
			<h3 ui:field="step">Confirm</h3>
		</g:HTMLPanel>
		<g:VerticalPanel>
			<g:HorizontalPanel>
				<g:Label text="Name" ui:field="name"/>
				<g:Label text="value" ui:field="nameValue"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Label text="First Name" ui:field="firstName"/>
				<g:Label text="value" ui:field="firstNameValue"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Label text="Email" ui:field="mail"/>
				<g:Label text="value" ui:field="mailValue"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Label text="PSA Entity" ui:field="psaEntity"/>
				<g:Label text="value" ui:field="psaEntityValue"/>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 